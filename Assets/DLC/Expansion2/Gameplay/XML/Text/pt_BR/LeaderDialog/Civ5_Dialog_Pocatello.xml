        <!--Demand for tribute from player - yes -->
        <Row Tag="TXT_KEY_LEADER_POCATELLO_TRIBUTE_YES_ANGRY">
            <Text>Eu vou conceder, mas eu nunca vou esquecer desse seu menosprezo contra o meu povo.</Text>
        </Row>
        <!--Demand for tribute from player - no -->
        <Row Tag="TXT_KEY_LEADER_POCATELLO_TRIBUTE_NO_ANGRY">
