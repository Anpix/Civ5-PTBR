        </Row>
        <Row Tag="TXT_KEY_LEADER_POCATELLO_AGREE_SHORT_2">
            <Text>Muito bem.</Text>
        </Row>
        <!--Disagrees -->
        <Row Tag="TXT_KEY_LEADER_POCATELLO_DISAGREE_SHORT_1">
